@font-face {
  font-family: 'Placard';
  src: url('fonts/placard/PlacardMT-BoldCondensed.eot');
  src: url('fonts/placard/PlacardMT-BoldCondensed.eot?#iefix') format('embedded-opentype'),
    url('fonts/placard/PlacardMT-BoldCondensed.woff2') format('woff2'),
    url('fonts/placard/PlacardMT-BoldCondensed.woff') format('woff'),
    url('fonts/placard/PlacardMT-BoldCondensed.ttf') format('truetype'),
    url('fonts/placard/PlacardMT-BoldCondensed.svg#PlacardMT-BoldCondensed') format('svg');
  font-weight: bold;
  font-style: normal;
}

